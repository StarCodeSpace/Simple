<Project>
  
  <PropertyGroup>
    <TargetFrameworks>netstandard2;net472</TargetFrameworks>
    <Authors>yxz</Authors>
    <Company></Company>
	<Git>https://gitee.com/VCodeWork/simple-framework</Git>
    <Copyright>Copyright @yxz 2021-2299</Copyright>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl>\</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <NeutralLanguage>zh-CN</NeutralLanguage>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|net472|AnyCPU'">
		<DefineConstants>NET47</DefineConstants>
		<OutputPath>bin\</OutputPath>
		<DocumentationFile>bin\net472\$(AssemblyName).xml</DocumentationFile>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|netstandard2|AnyCPU'">
		<OutputPath>bin\</OutputPath>
		<DocumentationFile>bin\netstandard2\$(AssemblyName).xml</DocumentationFile>
	</PropertyGroup>
</Project>